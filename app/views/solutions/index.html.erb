<p id="notice"><%= notice %></p>

<h1>Listing Solutions</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Correct</th>
      <th>Participant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solutions.each do |solution| %>
      <tr>
        <td><%= solution.question_id %></td>
        <td><%= solution.correct %></td>
        <td><%= solution.participant_id %></td>
        <td><%= link_to 'Show', solution %></td>
        <td><%= link_to 'Edit', edit_solution_path(solution) %></td>
        <td><%= link_to 'Destroy', solution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solution', new_solution_path %>
